<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.robertkovacs.mainViewController">
   <children>
      <VBox spacing="10.0">
         <children>
            <HBox alignment="CENTER">
               <VBox.margin>
                  <Insets top="100.0" />
               </VBox.margin>
               <children>
                  <TextField fx:id="inputField" alignment="CENTER" maxWidth="200.0" prefHeight="25.0" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" text="Code" onMouseClicked="#codeButtonClickHandle" />
               </children>
            </HBox>
            <HBox alignment="CENTER">
               <children>
                  <TextField fx:id="outputField" alignment="CENTER" maxWidth="400" prefHeight="25.0" prefWidth="400.0" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
